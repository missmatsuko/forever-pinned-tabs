{
  "manifest_version": 2,
  "name": "Forever Pinned Tabs",
  "description": "Keep pinned tabs pinned, forever.",
  "version": "1.0.0",
  "permissions": [
    "background",
    "storage",
    "tabs"
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "options_page": "options/options.html"
}
